{
	"compilerOptions": {
		"baseUrl": ".",
		"outDir": "build",
		"rootDir": "src",
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"typeRoots": ["node_modules/@types"],
		"esModuleInterop": true,
		"noUnusedLocals": false
	},
	"include": ["./src", "../secret"],
	"exclude": ["./node_modules"]
}
